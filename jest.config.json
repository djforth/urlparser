{
  "browser": true,
  "collectCoverageFrom": ["<rootDir>/src/packs/modules/**/*.{js,jsx}"],
  "coveragePathIgnorePatterns": [
    "<rootDir>/node_modules/",
    "<rootDir>/__tests__/",
    "<rootDir>/__tests__/__helpers__"
  ],
  "roots": ["<rootDir>/__tests__/", "<rootDir>/src/"],
  "modulePaths": ["<rootDir>/src/"],
  "modulePathIgnorePatterns": ["<rootDir>/lib/", "<rootDir>/node_modules/"],
  "moduleNameMapper": {
  },
  "transform": {
    "^.+\\.jsx$": "babel-jest",
    "^.+\\.js$": "babel-jest"
  },
  "setupTestFrameworkScriptFile": "@djforth/jest-matchers",
  "testPathIgnorePatterns": [
    "<rootDir>/__tests__/setupTests.js",
    "<rootDir>/__tests__/__dummy__",
    "<rootDir>/lib",
    "<rootDir>/node_modules"
  ],
  "unmockedModulePathPatterns": ["jasmine-expect"],
  "verbose": true
}
