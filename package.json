{
  "name": "@djforth/urlparser",
  "version": "2.0.0",
  "description": "URL parser to pull info from a url",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "build:watch": "NODE_ENV=production rollup -c -w",
    "lint": "eslint src/** --fix",
    "lint:tests": "eslint spec/** --fix",
    "test": "jest --coverage --config jest.config.json",
    "test:watch": "jest --watch --config jest.config.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djforth/urlparser.git"
  },
  "keywords": [
    "URL Parser",
    "URI Parser"
  ],
  "author": "Adrian Stainforth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djforth/urlparser/issues"
  },
  "assets": {
    "assets_in": "./src",
    "karma": {
      "input": "./spec",
      "specs": "/*_spec.js"
    }
  },
  "homepage": "https://github.com/djforth/urlparser",
  "devDependencies": {
    "@djforth/jest-matchers": "^1.5.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-lodash": "^3.3.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.4.0",
    "browserslist": "^3.2.6",
    "eslint": "^4.19.1",
    "eslint-config-morsedigital-prettier": "^1.2.2",
    "eslint-plugin-import": "^2.11.0",
    "jest": "^22.4.3",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.2",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.4"
  }
}
