{
  "name": "@djforth/urlparser",
  "version": "2.0.0",
  "description": "URL parser to pull info from a url",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "build": "NODE_ENV=production babel src/urlparser.js --source-maps  --out-file index.js",
    "build:watch": "NODE_ENV=production babel src/urlparser.js -w --source-maps  --out-file index.js",
    "lint": "eslint src/** --fix",
    "lint:tests": "eslint __tests__/** --fix",
    "test": "asset-pipeline-karma",
    "test:watch": "asset-pipeline-karma -w",
    "test:mac": "asset-pipeline-karma -b"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djforth/urlparser.git"
  },
  "keywords": [
    "URL Parser",
    "URI Parser"
  ],
  "author": "Adrian Stainforth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djforth/urlparser/issues"
  },
  "assets": {
    "assets_in": "./src",
    "karma": {
      "input": "./spec",
      "specs": "/*_spec.js"
    }
  },
  "homepage": "https://github.com/djforth/urlparser",
  "devDependencies": {
    "@djforth/ap_karma_wp": "^3.0.2",
    "@djforth/ap_utils": "^3.1.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-rewire-ignore-coverage": "^2.0.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.2.0",
    "browserslist": "^2.5.1",
    "eslint": "^4.9.0",
    "eslint-config-morsedigital": "^2.1.1",
    "jasmine-core": "^2.8.0",
    "lodash-webpack-plugin": "^0.11.4"
  },
  "dependencies": {
    "lodash": "^4.17.4"
  }
}
