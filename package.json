{
  "name": "@djforth/urlparser",
  "version": "1.1.2",
  "description": "URL parser to pull info from a url",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "build": "babel src -x '.js'  -d lib",
    "build:watch": "babel src -x '.js' -w --source-maps  -d lib",
    "lint": "eslint src/**",
    "lint:tests": "eslint spec/**",
    "test": "asset-pipeline-karma",
    "test:watch": "asset-pipeline-karma -w",
    "test:mac": "asset-pipeline-karma -b"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djforth/urlparser.git"
  },
  "keywords": [
    "URL Parser",
    "URI Parser"
  ],
  "author": "Adrian Stainforth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djforth/urlparser/issues"
  },
  "assets": {
    "assets_in": "./src",
    "karma": {
      "input": "./spec",
      "specs": "/*_spec.js"
    }
  },
  "homepage": "https://github.com/djforth/urlparser",
  "devDependencies": {
    "@djforth/ap_karma": "1.1.8",
    "@djforth/ap_utils": "^2.0.13",
    "@djforth/morse-jasmine": "1.0.2",
    "babel": "^6.5.2",
    "babel-cli": "^6.6.5",
    "babel-eslint": "^5.0.0",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "eslint": "^2.2.0",
    "eslint-config-google": "^0.4.0",
    "lodash": "^4.6.1"
  },
  "peerDependencies": {
    "babel-polyfill": "^6.6.4",
    "lodash": "^4.6.1"
  }
}
